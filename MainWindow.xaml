<Window x:Class="DockPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DockPanel"
        mc:Ignorable="d"
        Title="DockPanel" Height="150" Width="250">
    <DockPanel LastChildFill="False">
        <Button Content="domyslny" Margin="2"/>
        <Button Content="Góra 1" Margin="2" DockPanel.Dock="top"/>
        <Button Content="Góra 2" Margin="2" DockPanel.Dock="top"/>
        <Button Content="Dół" Margin="2" DockPanel.Dock="Bottom"/>
        <Button Content="Prawy" Margin="2" DockPanel.Dock="Right"/>
        <Button Content="OSTATNI" Margin="2"/>
    </DockPanel>
</Window>
